{
    "name": "your-vendor/ucp-checkout-for-woocommerce",
    "description": "UCP Checkout for WooCommerce - Enable AI agents to purchase from your store via UCP",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "UcpPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.4"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0"
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
